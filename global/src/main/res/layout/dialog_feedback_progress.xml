<?xml version="1.0" encoding="utf-8"?>
<!--laughing  多个页面共用不可以删除-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              style="@style/style_bottom_dialog_ll_outer">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="30px"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_set_priority"
            style="@style/style_bottom_dialog_tv"
            android:background="@mipmap/btn_top_arc"
            android:text="设置优先级"/>

        <ImageView
            style="@style/style_bottom_dialog_line"/>

        <TextView
            android:id="@+id/tv_feedback_progress"
            style="@style/style_bottom_dialog_tv"
            android:background="@mipmap/btn_bottom_arc"
            android:text="反馈任务进度"/>
    </LinearLayout>

    <TextView
        android:id="@+id/tv_cancel"
        style="@style/style_bottom_dialog_tv"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="30px"
        android:layout_marginRight="30px"
        android:layout_marginTop="20px"
        android:background="@mipmap/btn_top_arc"
        android:text="取消"/>

</LinearLayout>